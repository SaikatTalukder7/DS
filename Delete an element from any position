#include <iostream>
using namespace std;

int main()
{
    int n,pos;
    cout<<"Enter the range of array: ";
    cin>>n;
    int arr[n];
    cout<<"Enter the elements of array: ";
    for(int i=0; i<n; i++)
    {
        cin>>arr[i];
    }
    cout<<"Enter the position that want to delete: ";
    cin>>pos;
    for (int i = pos-1; i < n - 1; i++)
     {
        arr[i] = arr[i + 1];
     }
    n--;
    cout<<"After deletion from specific position: ";
    for (int i = 0; i < n; i++)
     {
        cout << arr[i] << " ";
     }
    cout << endl;

    return 0;
}
