#include<bits/stdc++.h>
#include<iostream>
using namespace std;
int main()
{
    int n,value;
    cout<<"enter the rang of array";
    cin>>n;
    int a[n+1];
    cout<<"enter the element of array";
    for(int i=0; i<n; i++)
    {
        cin>>a[i];
    }
    cin>>value;
    a[n]=value;
    n++;
    cout<<"After insertion at the end";
    for(int i=0; i<n; i++)
    {
        cout<<a[i]<<" ";
    }
    return 0;
}
